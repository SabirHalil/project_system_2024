					"const echoPostRequest = {",
					"  url: pm.collectionVariables.get(\"url\")+'/api/v1/account/token/',",
					"  method: 'POST',",
					"     header: {",
					"        'content-type': 'application/json',",
					"    },",
					"    body: {",
					"        mode: 'raw',",
					"        raw: JSON.stringify({ email: pm.collectionVariables.get(\"email\"), password: pm.collectionVariables.get(\"password\") })",
					"    }",
					"};",
					"pm.sendRequest(echoPostRequest, function (err, res) {",
					"    const json = res.json() // Get JSON value from the response body ",
					"    console.log(json)",
					"    pm.collectionVariables.set(\"token\", json.data.access) // makes sure to update this key name.",
					"});"
				]
			}
